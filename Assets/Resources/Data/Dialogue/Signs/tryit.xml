<?xml version="1.0" encoding="utf-8"?>

<DialogueTree>
	<Dialogue>
	
		<Speech Name="Sign">
			<PID>1</PID>
			<Content>(There's a stone with an arrow on it.)</Content>
		</Speech>
		
		<Speech Name="Sign">
			<Type>1</Type>
			<PID>1</PID>
			<Content>(Someone has inscribed a message. Read it?)</Content>
			<OB>QYN</OB>
		</Speech>

		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>Jump with X and Throw with C.</Content>
		</Speech>

		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>Hold C to keep it spinning.</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>Press A while it's out to fly.</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>You'll never be 'forced' to do this.</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>Easy to get through here without the move.</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>But go on, give it a go.</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>It is MUCH faster than walking.</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>By the waaaay...</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>Definitely fly out of bounds.</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>You'll never regret it.</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>Nothing bad could ever happen.</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>Scout's honor.</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>- Dr. Saturday</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>(Saturday seems like an odd fellow.)</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Content>(That last bit there, though...)</Content>
		</Speech>

		<Speech Name="Sign" Branch="QYN0">
			<PID>1</PID>
			<Type>5</Type>
			<Content>(Sounds like a trick.)</Content>
		</Speech>
		
		<Speech Name="Sign" Branch="QYN1">
			<PID>1</PID>
			<Type>5</Type>
			<Content>(Should probably read it, though.)</Content>
		</Speech>
	
	</Dialogue>
	
